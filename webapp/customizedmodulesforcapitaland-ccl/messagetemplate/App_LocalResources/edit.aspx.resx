                                &lt;td style="width: 30px" __designer:mapid="1e8"&gt;
                                &lt;/td&gt;
                                &lt;td __designer:mapid="1e9"&gt;
                                    
                                        Dear {obj.Supervisor.ObjectName},&lt;br __designer:mapid="1ea" /&gt;
                                        &lt;br __designer:mapid="1eb" /&gt;
                                        The following work order has been submitted to you for approval.&lt;br 
                                        __designer:mapid="1ec" /&gt;
                                        &lt;br __designer:mapid="1ed" /&gt;
                                        WR#: {obj.ObjectNumber}&lt;br __designer:mapid="1ee" /&gt;
                                        Description: {obj.WorkDescription}&lt;br __designer:mapid="1ef" /&gt;
                                        Type of Work: {obj.TypeOfWork.ObjectName}&lt;br __designer:mapid="1f0" /&gt;
                                        Type of Service: {obj.TypeOfService.ObjectName}&lt;br __designer:mapid="1f1" /&gt;
                                        &lt;br __designer:mapid="1f2" /&gt;
                                        &lt;br __designer:mapid="1f3" /&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                        &lt;/table&gt;
                        &lt;br __designer:mapid="1f4" /&gt;
                        &lt;br __designer:mapid="1f5" /&gt;
                        The following are The following variables are available for tagging:
                        &lt;br __designer:mapid="1f6" /&gt;
                        &lt;br __designer:mapid="1f7" /&gt;
                        &lt;table style="width: 100%" __designer:mapid="1f8"&gt;
                            &lt;tr __designer:mapid="1f9"&gt;
                                &lt;td style="width: 30px" __designer:mapid="1fa"&gt;
                                &lt;/td&gt;
                                &lt;td __designer:mapid="1fb"&gt;
                                    {obj.????}&lt;br __designer:mapid="1fc"&gt;&lt;/br&gt;
                                    {applicationSettings.????}&lt;br __designer:mapid="1fd"&gt;&lt;/br&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                        &lt;/table&gt;
                        &lt;br __designer:mapid="1fe" /&gt;
                        &lt;br __designer:mapid="1ff" /&gt;
