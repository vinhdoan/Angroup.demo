    /// <summary>
    /// Sets up the grid to show the budget line item.
    /// </summary>
    /// <param name="sender"></param>
    /// <param name="e"></param>
    void grid_ItemDataBound(object sender, DataGridItemEventArgs e)
    {
        DataGrid grid = sender as DataGrid;

        // Hide the intervals that are greater than the total
        // number of intervals in this budget period
        //
        if (sender is DataGrid && e.Item.ItemType == ListItemType.Header)
        {
            int numberOfIntervals = budgetPeriod.TotalNumberOfIntervals;
            int monthsPerInterval = budgetPeriod.NumberOfMonthsPerInterval.Value;
            for (int i = 0; i < grid.Columns.Count; i++)
            {
                if (grid.Columns[i] is BoundColumn &&
                    ((BoundColumn)grid.Columns[i]).DataField.StartsWith("Interval") &&
                    ((BoundColumn)grid.Columns[i]).DataField.EndsWith("Amount"))
                {
                    int interval = Convert.ToInt32(((BoundColumn)grid.Columns[i]).DataField
                        .Replace("Interval", "").Replace("Amount", ""));
                    if (interval > numberOfIntervals)
                        grid.Columns[i].Visible = false;
                    else
                    {
                        DateTime intervalStartDate = budgetPeriod.StartDate.Value.AddMonths((interval - 1) * monthsPerInterval);
                        if (intervalStartDate.Day == 1)
                            e.Item.Cells[i].Text =
                                intervalStartDate.ToString("MMM-yyyy");
                        else
                            e.Item.Cells[i].Text =
                                intervalStartDate.ToString("dd-MMM-yyyy");
                    }
                }
            }
        }

        if (e.Item.ItemType == ListItemType.Item || e.Item.ItemType == ListItemType.AlternatingItem)
        {
            // if the Account is not a line item type, 
            // set the font of the Account to bold
            //
            if (Convert.ToInt32(e.Item.Cells[2].Text) == 0)
                e.Item.Cells[3].Font.Bold = true;

            // Adding padding to simulate a tree effect.
            //
            int level = Convert.ToInt32(e.Item.Cells[1].Text);
            e.Item.Cells[3].Text = new StringBuilder().Insert(0, "&nbsp; ", level).ToString() +
                e.Item.Cells[3].Text.ToString();

            // Show the currency (and highlight in red if negative.
            // 
            for (int i = 0; i < grid.Columns.Count; i++)
            {
                if (grid.Columns[i] is BoundColumn &&
                    (((BoundColumn)grid.Columns[i]).DataField.StartsWith("Total") ||
                    ((BoundColumn)grid.Columns[i]).DataField.StartsWith("Interval")))
                {
                    try
                    {

                        if (e.Item.Cells[i].Text != "" &&
                            e.Item.Cells[i].Text != "&nbsp;")
                        {
                            if (Convert.ToDecimal(e.Item.Cells[i].Text) < 0)
                            {
                                e.Item.Cells[i].ForeColor = System.Drawing.Color.Red;
                                e.Item.Cells[i].Text = Convert.ToDecimal(e.Item.Cells[i].Text).ToString("n");
                            }
                            else
                            {
                                e.Item.Cells[i].Text = Convert.ToDecimal(e.Item.Cells[i].Text).ToString("n");
                            }
                        }
                    }
                    catch
                    {
                        i = i;
                    }
                }
            }
            if (Convert.ToInt32(e.Item.Cells[2].Text) == 1)
            {
                HyperLink account = (HyperLink)e.Item.FindControl("linkAccount");
                account.Text = e.Item.Cells[3].Text;
                account.NavigateUrl = "WJDetails.aspx?AccountID=" + HttpUtility.UrlEncode(Security.Encrypt(e.Item.Cells[0].Text)) +
                    "&BudgetPeriodID=" + HttpUtility.UrlEncode(Security.Encrypt(BudgetPeriod.ObjectID.ToString()));
                Label lblAccount = (Label)e.Item.FindControl("lblAccount");
                lblAccount.Visible = false;
            }
            else
            {
                HyperLink account = (HyperLink)e.Item.FindControl("linkAccount");
                account.Visible = false;
                Label lblAccount = (Label)e.Item.FindControl("lblAccount");
                lblAccount.Text = e.Item.Cells[3].Text;
                lblAccount.Font.Bold = true;   
                lblAccount.Visible = true;
            }
        }
    }


