  <DataSets>
    <DataSet Name="abell">
      <Query>
        <DataSourceName>Shared</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StoreID">
            <Value>=Parameters!StoreID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TreeviewID">
            <Value>=Parameters!TreeviewID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     store.ObjectName AS Store, { fn CONCAT(CASE WHEN location8.ObjectName IS NULL THEN '' ELSE { fn concat(location8.ObjectName, ' &gt; ') } END, 
                      { fn CONCAT(CASE WHEN location7.ObjectName IS NULL THEN '' ELSE { fn concat(location7.ObjectName, ' &gt; ') } END, 
                      { fn CONCAT(CASE WHEN location6.ObjectName IS NULL THEN '' ELSE { fn concat(location6.ObjectName, ' &gt; ') } END, 
                      { fn CONCAT(CASE WHEN location5.ObjectName IS NULL THEN '' ELSE { fn concat(location5.ObjectName, ' &gt; ') } END, 
                      { fn CONCAT(CASE WHEN location4.ObjectName IS NULL THEN '' ELSE { fn concat(location4.ObjectName, ' &gt; ') } END, 
                      { fn CONCAT(CASE WHEN location3.ObjectName IS NULL THEN '' ELSE { fn concat(location3.ObjectName, ' &gt; ') } END, 
                      { fn CONCAT(CASE WHEN location2.ObjectName IS NULL THEN '' ELSE { fn concat(location2.ObjectName, ' &gt; ') } END, 
                      { fn CONCAT(CASE WHEN location1.ObjectName IS NULL THEN '' ELSE { fn concat(location1.ObjectName, ' &gt; ') } END, location.ObjectName) }) }) }) }) }) }) 
                      }) } AS Location, catalogue.ObjectName AS Catalog, 
                      CASE WHEN storeItem.ItemType = 0 THEN 'Stocked' WHEN storeItem.ItemType = 1 THEN 'Non-Stocked' END AS 'Item Type', 
                      CASE WHEN storeItem.CostingType = 0 THEN 'FIFO' WHEN storeItem.ItemType = 1 THEN 'LIFO' END AS 'Costing Type', 
                      unitOfMeasure.ObjectName AS UOM, storeItem.ReorderDefault AS 'Reorder Quantity', storeItem.ReorderThreshold AS 'Reorder Threshold',
                          (SELECT     SUM(CASE WHEN storeBinItem.PhysicalQuantity IS NULL THEN 0 ELSE storeBinItem.PhysicalQuantity END)
                            FROM          storeBinItem LEFT JOIN
                                                   storeBin ON storeBin.ObjectID = storeBinItem.StoreBinID
                            WHERE      storeBinItem.IsDeleted = 0 AND storeBinItem.CatalogueID = storeItem.CatalogueID AND storeBin.StoreID = store.ObjectID) 
                      - CASE WHEN
                          (SELECT     SUM(CASE WHEN storeBinReservation.BaseQuantityReserved IS NULL THEN 0 ELSE storeBinReservation.BaseQuantityReserved END)
                            FROM          storeBinReservation LEFT JOIN
                                                   storeBin ON storeBin.ObjectID = storeBinReservation.StoreBinID
                            WHERE      storeBinReservation.IsDeleted = 0 AND storeBin.StoreID = store.ObjectID AND 
                                                   storeBinReservation.CatalogueID = storeItem.CatalogueID AND storeBinReservation.BaseQuantityReserved &gt; 0) IS NULL 
                      THEN 0 ELSE
                          (SELECT     SUM(CASE WHEN storeBinReservation.BaseQuantityReserved IS NULL THEN 0 ELSE storeBinReservation.BaseQuantityReserved END)
                            FROM          storeBinReservation LEFT JOIN
                                                   storeBin ON storeBin.ObjectID = storeBinReservation.StoreBinID
                            WHERE      storeBinReservation.IsDeleted = 0 AND storeBin.StoreID = store.ObjectID AND 
                                                   storeBinReservation.CatalogueID = storeItem.CatalogueID AND storeBinReservation.BaseQuantityReserved &gt; 0) 
                      END AS 'Available Balance',
                          (SELECT     SUM(CASE WHEN storeBinItem.PhysicalQuantity IS NULL THEN 0 ELSE storeBinItem.PhysicalQuantity END)
                            FROM          storeBinItem LEFT JOIN
                                                   storeBin ON storeBin.ObjectID = storeBinItem.StoreBinID
                            WHERE      storeBinItem.IsDeleted = 0 AND storeBinItem.CatalogueID = storeItem.CatalogueID AND storeBin.StoreID = store.ObjectID) 
                      AS 'Physical Balance',
                          (SELECT     SUM(CASE WHEN storeBinItem.PhysicalQuantity * storeBinItem.UnitPrice IS NULL 
                                                   THEN 0 ELSE storeBinItem.PhysicalQuantity * storeBinItem.UnitPrice END)
                            FROM          storeBinItem LEFT JOIN
                                                   storeBin ON storeBin.ObjectID = storeBinItem.StoreBinID
                            WHERE      storeBinItem.IsDeleted = 0 AND storeBinItem.CatalogueID = storeItem.CatalogueID AND storeBin.StoreID = store.ObjectID) 
                      AS 'Physical Cost'
FROM         StoreItem storeItem LEFT OUTER JOIN
                      Catalogue catalogue ON catalogue.ObjectID = storeItem.CatalogueID LEFT OUTER JOIN
                      Code unitOfMeasure ON unitOfMeasure.ObjectID = catalogue.UnitOfMeasureID LEFT OUTER JOIN
                      Store store ON store.ObjectID = storeItem.StoreID LEFT OUTER JOIN
                      Location location ON location.ObjectID = store.LocationID LEFT OUTER JOIN
                      Location location1 ON location1.ObjectID = location.ParentID LEFT OUTER JOIN
                      Location location2 ON location2.ObjectID = location1.ParentID LEFT OUTER JOIN
                      Location location3 ON location3.ObjectID = location2.ParentID LEFT OUTER JOIN
                      Location location4 ON location4.ObjectID = location3.ParentID LEFT OUTER JOIN
                      Location location5 ON location5.ObjectID = location4.ParentID LEFT OUTER JOIN
                      Location location6 ON location6.ObjectID = location5.ParentID LEFT OUTER JOIN
                      Location location7 ON location7.ObjectID = location6.ParentID LEFT OUTER JOIN
                      Location location8 ON location8.ObjectID = location7.ParentID
WHERE     (storeItem.IsDeleted = 0) AND (store.IsDeleted = 0) AND ((CASE WHEN @StoreID = '' OR
                      @StoreID IS NULL THEN 1 WHEN store.ObjectID = @StoreID THEN 1 ELSE 0 END) = 1) AND ((CASE WHEN @TreeviewID IS NULL 
                      THEN 1 WHEN location.HierarchyPath LIKE { fn concat
                          ((SELECT     HierarchyPath
                              FROM         Location
                              WHERE     ObjectID = @TreeviewID), '%') } THEN 1 ELSE 0 END) = 1)
ORDER BY store.ObjectName</CommandText>
      </Query>
